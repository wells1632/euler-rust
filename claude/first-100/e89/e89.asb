10 REM ROMAN NUMERAL MINIMIZER
20 REM ANALYZE ROMAN NUMERALS AND FIND CHARACTER SAVINGS
30 GOSUB 10000: REM INITIALIZE
40 GOSUB 20000: REM PROCESS DATA
50 GOSUB 30000: REM SHOW RESULTS
60 END
10000 REM INITIALIZE VARIABLES
10010 TS = 0: REM TOTAL SAVINGS
10020 LC = 0: REM TOKEN COUNT
10030 EX = 0: REM EXAMPLE COUNT
10040 DIM EO$(10),EM$(10),ES(10): REM EXAMPLE ARRAYS
10050 RETURN
20000 REM PROCESS ALL DATA
20010 READ R$
20020 IF R$ = "END" THEN RETURN
20030 LC = LC + 1
20040 REM CONVERT TO DECIMAL
20050 GOSUB 21000
20060 DV = RV: REM SAVE DECIMAL VALUE
20070 REM CONVERT BACK TO MINIMAL
20080 GOSUB 22000
20090 OL = LEN(R$): REM ORIGINAL LENGTH
20100 ML = LEN(M$): REM MINIMAL LENGTH
20110 SV = OL - ML: REM SAVINGS
20120 IF SV > 0 THEN TS = TS + SV
20150 GOTO 20010
21000 REM CONVERT ROMAN TO DECIMAL
21010 REM INPUT: R$ OUTPUT: RV
21020 RV = 0
21030 PV = 0: REM PREVIOUS VALUE
21040 FOR I = LEN(R$) TO 1 STEP -1
21050 C$ = MID$(R$,I,1)
21060 V = 0
21070 IF C$ = "I" THEN V = 1
21080 IF C$ = "V" THEN V = 5
21090 IF C$ = "X" THEN V = 10
21100 IF C$ = "L" THEN V = 50
21110 IF C$ = "C" THEN V = 100
21120 IF C$ = "D" THEN V = 500
21130 IF C$ = "M" THEN V = 1000
21140 IF V < PV THEN RV = RV - V: GOTO 21160
21150 RV = RV + V
21160 PV = V
21170 NEXT I
21180 RETURN
22000 REM CONVERT DECIMAL TO MINIMAL ROMAN
22010 REM INPUT: DV OUTPUT: M$
22020 M$ = ""
22030 N = DV
22040 REM 1000 = M
22050 GOSUB 23000: VA = 1000: RS$ = "M": GOSUB 23100
22060 REM 900 = CM
22070 GOSUB 23000: VA = 900: RS$ = "CM": GOSUB 23100
22080 REM 500 = D
22090 GOSUB 23000: VA = 500: RS$ = "D": GOSUB 23100
22100 REM 400 = CD
22110 GOSUB 23000: VA = 400: RS$ = "CD": GOSUB 23100
22120 REM 100 = C
22130 GOSUB 23000: VA = 100: RS$ = "C": GOSUB 23100
22140 REM 90 = XC
22150 GOSUB 23000: VA = 90: RS$ = "XC": GOSUB 23100
22160 REM 50 = L
22170 GOSUB 23000: VA = 50: RS$ = "L": GOSUB 23100
22180 REM 40 = XL
22190 GOSUB 23000: VA = 40: RS$ = "XL": GOSUB 23100
22200 REM 10 = X
22210 GOSUB 23000: VA = 10: RS$ = "X": GOSUB 23100
22220 REM 9 = IX
22230 GOSUB 23000: VA = 9: RS$ = "IX": GOSUB 23100
22240 REM 5 = V
22250 GOSUB 23000: VA = 5: RS$ = "V": GOSUB 23100
22260 REM 4 = IV
22270 GOSUB 23000: VA = 4: RS$ = "IV": GOSUB 23100
22280 REM 1 = I
22290 GOSUB 23000: VA = 1: RS$ = "I": GOSUB 23100
22300 RETURN
23000 REM PLACEHOLDER FOR SETTING VA AND RS$
23010 RETURN
23100 REM SUBTRACT VALUE AND APPEND STRING
23110 IF N >= VA THEN M$ = M$ + RS$: N = N - VA: GOTO 23110
23120 RETURN
30000 REM SHOW RESULTS
30010 PRINT "ROMAN NUMERAL ANALYSIS"
30020 PRINT "======================"
30030 PRINT "TOKENS PROCESSED: ";LC
30040 PRINT "TOTAL CHARACTERS SAVED: ";TS
30050 RETURN
