10  REM F(50,N) SEARCH
20  PRINT "FINDING F(50,N) > 1000000"
30  PRINT
40  M = 50:T = 1000000
50  REM TEST FIRST
60  M = 3:N = 7: GOSUB 1000
70  PRINT "TEST: F(3,7) = ";W
80  IF W = 17 THEN  PRINT "OK!": GOTO 100
90  PRINT "FAILED!": END
100  PRINT
110  M = 50:N = 50
120  PRINT "SEARCHING..."
130  GOSUB 1000
140  IF W > T THEN  GOTO 500
150  IF N / 10 = INT (N / 10) THEN  PRINT "N=";N;" W=";W
160  N = N + 1
170  GOTO 130
500  PRINT
510  PRINT "FOUND!"
520  PRINT "N = ";N
530  PRINT "F(50,";N;") = ";W
540  END
1000  REM CALCULATE F(M,N)
1010  DIM F(210)
1020  F(0) = 1
1030  FOR L = 1 TO N
1040  F(L) = F(L - 1)
1050  FOR K = M TO L
1060  IF K = L THEN F(L) = F(L) + 1: GOTO 1080
1070  F(L) = F(L) + F(L - K - 1)
1080  NEXT K
1090  NEXT L
1100  W = F(N)
1110  RETURN