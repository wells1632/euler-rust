10 REM FIND SMALLEST ODD COMPOSITE THAT CANNOT BE WRITTEN AS PRIME + 2*SQUARE
20 PRINT "FINDING SMALLEST ODD COMPOSITE..."
30 N = 9: REM START WITH FIRST ODD COMPOSITE
40 REM CHECK IF N IS COMPOSITE (NOT PRIME)
50 X = N: GOSUB 200
60 IF PR = 1 THEN GOTO 140: REM SKIP IF PRIME
70 REM CHECK IF N CAN BE WRITTEN AS PRIME + 2*SQUARE
80 GOSUB 300
90 IF CW = 0 THEN GOTO 110: REM FOUND ANSWER
100 GOTO 140: REM CONTINUE SEARCHING
110 PRINT "FOUND: "; N
120 PRINT "VERIFICATION - CHECKING DECOMPOSITIONS:"
130 GOSUB 400: REM SHOW VERIFICATION
140 N = N + 2: REM NEXT ODD NUMBER
150 IF N <= 10000 THEN GOTO 40
160 PRINT "SEARCHED UP TO "; N; ", NO SOLUTION FOUND"
170 END
200 REM CHECK IF X IS PRIME
210 PR = 0: IF X < 2 THEN RETURN
220 IF X = 2 THEN PR = 1: RETURN
230 IF X - 2 * INT(X / 2) = 0 THEN RETURN
240 I = 3
250 IF I * I > X THEN PR = 1: RETURN
260 IF X - I * INT(X / I) = 0 THEN RETURN
270 I = I + 2: GOTO 250
300 REM CHECK IF N CAN BE WRITTEN AS PRIME + 2*SQUARE
310 CW = 0: K = 1
320 IF 2 * K * K >= N THEN RETURN
330 RE = N - 2 * K * K
340 X = RE: GOSUB 200
350 IF PR = 1 THEN CW = 1: RETURN
360 K = K + 1: GOTO 320
400 REM SHOW VERIFICATION
410 K = 1
420 IF 2 * K * K >= N THEN RETURN
430 RE = N - 2 * K * K
440 X = RE: GOSUB 200
450 PRINT "  "; N; " - 2*"; K; "Â² = "; RE; " (PRIME: ";
460 IF PR = 1 THEN PRINT "YES)": GOTO 480
470 PRINT "NO)"
480 K = K + 1: GOTO 420