10 REM FIND DIGITS IN CONCATENATED INTEGER SEQUENCE
20 DIM PO(7): DIM DI(7)
30 REM SET POSITIONS TO FIND
40 PO(1) = 1: PO(2) = 10: PO(3) = 100: PO(4) = 1000
50 PO(5) = 10000: PO(6) = 100000: PO(7) = 1000000
60 PRINT "FINDING DIGITS AT POSITIONS:"
70 FOR I = 1 TO 7
80   GOSUB 200: REM GET DIGIT AT POSITION PO(I)
90   DI(I) = DT
100   PRINT "POSITION "; PO(I); ": "; DI(I)
110 NEXT I
120 REM CALCULATE PRODUCT
130 PR = 1
140 FOR I = 1 TO 7
150   PR = PR * DI(I)
160 NEXT I
170 PRINT : PRINT "PRODUCT OF ALL DIGITS: "; PR
180 END
200 REM GET DIGIT AT POSITION PO(I)
210 CP = 0: NU = 1
220 NL = LEN(STR$(NU))
230 IF CP + NL >= PO(I) THEN GOTO 270
240 CP = CP + NL
250 NU = NU + 1
260 GOTO 220
270 REM DIGIT IS WITHIN NUMBER NU
280 DX = PO(I) - CP - 1
290 NS$ = STR$(NU)
300 DT = VAL(MID$(NS$, DX + 1, 1))
310 RETURN